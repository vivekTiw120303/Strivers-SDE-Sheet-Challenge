#include <bits/stdc++.h>

vector<vector<long long int>> printPascal(int n) 
{
  vector<vector<long long>>ans(n);
  for(int i=0;i<n;i++)
  {
      ans[i].resize(i+1,1);
      if(i==0 || i==1) continue;

      for(int j=1;j<i;j++)
      {
          ans[i][j]=ans[i-1][j-1]+ans[i-1][j];
      }
  }
  return ans;

}
